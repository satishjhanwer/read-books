{
  "name": "read-books",
  "version": "2.0.0",
  "description": "A sample React application using Good Books API",
  "repository": {
    "type": "git",
    "url": "git://github.com/satishjhanwer/read-books.git"
  },
  "engines": {
    "npm": "6.14.11",
    "node": "12.21.0"
  },
  "author": "Satish Jhanwer",
  "license": "MIT",
  "scripts": {
    "postversion": "git push --tags",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "npm run start:prod",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "npm run lint:eslint:fix",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "chalk": "4.1.0",
    "compression": "1.7.4",
    "connected-react-router": "6.9.1",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "fontfaceobserver": "2.1.0",
    "history": "5.0.0",
    "hoist-non-react-statics": "3.3.2",
    "immer": "8.0.1",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "lodash": "4.17.21",
    "minimist": "1.2.5",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-intl": "5.13.2",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "reactstrap": "8.9.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "reselect": "4.0.0",
    "sanitize.css": "12.0.1",
    "styled-components": "5.2.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.13.8",
    "@babel/plugin-transform-react-constant-elements": "7.13.10",
    "@babel/plugin-transform-react-inline-elements": "7.12.13",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@babel/register": "7.13.8",
    "add-asset-html-webpack-plugin": "3.2.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "8.2.25",
    "babel-plugin-styled-components": "1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "5.2.2",
    "compare-versions": "3.6.0",
    "compression-webpack-plugin": "7.1.2",
    "coveralls": "3.1.0",
    "css-loader": "5.1.2",
    "eslint": "7.22.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-redux-saga": "1.2.1",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "image-webpack-loader": "7.0.1",
    "imports-loader": "2.0.0",
    "jest-cli": "26.6.3",
    "jest-dom": "4.0.0",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.4",
    "node-plop": "0.26.2",
    "null-loader": "4.0.1",
    "offline-plugin": "5.0.7",
    "plop": "2.7.4",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "react-app-polyfill": "2.0.0",
    "react-test-renderer": "17.0.1",
    "react-testing-library": "8.0.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "style-loader": "2.0.0",
    "stylelint": "13.12.0",
    "stylelint-config-recommended": "4.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "5.1.1",
    "url-loader": "4.1.1",
    "webpack": "5.25.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-middleware": "4.1.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-pwa-manifest": "4.3.0",
    "whatwg-fetch": "3.6.2"
  }
}
