{
  "name": "read-books",
  "version": "2.0.0",
  "description": "A sample React application using Good Books API",
  "repository": {
    "type": "git",
    "url": "git://github.com/satishjhanwer/read-books.git"
  },
  "engines": {
    "node": ">=22"
  },
  "author": "Satish Jhanwer",
  "license": "MIT",
  "scripts": {
    "postversion": "git push --tags",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color --progress --stats-children --stats-error-details",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "npm run lint:eslint:fix",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "connected-react-router": "6.9.2",
    "fontfaceobserver": "2.3.0",
    "history": "5.3.0",
    "hoist-non-react-statics": "3.3.2",
    "immer": "8.0.1",
    "invariant": "2.2.4",
    "lodash": "4.17.21",
    "minimist": "1.2.6",
    "prop-types": "15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-helmet": "6.1.0",
    "react-intl": "5.13.2",
    "react-redux": "7.2.8",
    "react-router-dom": "6.3.0",
    "reactstrap": "8.9.0",
    "redux": "4.2.0",
    "redux-first-history": "5.0.9",
    "redux-saga": "1.1.3",
    "reduxjs-toolkit-persist": "7.2.1",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "reselect": "4.1.5",
    "sanitize.css": "13.0.0",
    "styled-components": "5.3.5",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@babel/polyfill": "7.12.1",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.18.2",
    "@babel/plugin-transform-react-constant-elements": "7.17.12",
    "@babel/plugin-transform-react-inline-elements": "7.16.7",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/register": "7.17.7",
    "@redux-devtools/extension": "^3.2.2",
    "add-asset-html-webpack-plugin": "3.2.0",
    "babel-loader": "8.2.5",
    "chalk": "5.4.1",
    "compression": "1.7.4",
    "circular-dependency-plugin": "5.2.2",
    "compare-versions": "4.1.3",
    "compression-webpack-plugin": "9.2.0",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
