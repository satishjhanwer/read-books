{
  "name": "read-books",
  "version": "2.0.0",
  "description": "A sample React application using Good Books API",
  "repository": {
    "type": "git",
    "url": "git://github.com/satishjhanwer/read-books.git"
  },
  "engines": {
    "node": ">=22"
  },
  "author": "Satish Jhanwer",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "prebuild": "rimraf ./build",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "pretest": "rimraf ./coverage",
    "clean": "yarn prebuild && yarn pretest",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "test": "cross-env NODE_ENV=test jest --coverage --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --no-coverage",
    "prettify": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --color --progress --stats-children --stats-error-details"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "lint-staged": {
    "*.js": [
      "yarn lint"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
